{
  "name": "ai-life-assistant",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "vite",
    "web:build": "vite build",
    "web:preview": "vite preview",
    "start": "react-native start",
    "test": "jest",
    "test:ci": "jest --runInBand --passWithNoTests",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "validate:agent": "npm run type-check && npm run test:ci",
    "e2e:build:android": "detox build -c android.emu.debug",
    "e2e:test:android": "detox test -c android.emu.debug --headless",
    "e2e:android": "npm run e2e:build:android && npm run e2e:test:android",
    "android:clean": "cd android && gradlew clean && cd ..",
    "start:android": "npm start"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-community/datetimepicker": "^7.7.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^2.0.1",
    "axios": "^1.6.5",
    "date-fns": "^3.2.0",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.73.2",
    "react-native-gesture-handler": "2.14.0",
    "react-native-paper": "^5.12.1",
    "react-native-reanimated": "3.6.2",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "3.29.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "^0.19.10",
    "react-native-webview": "^13.12.5",
    "react-redux": "^9.0.4",
    "realm": "12.3.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/runtime": "^7.23.7",
    "@react-native/babel-preset": "0.73.19",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.3",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.47",
    "@types/react-native": "^0.73.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "@vitejs/plugin-react": "^5.1.4",
    "babel-plugin-module-resolver": "^5.0.2",
    "detox": "^20.16.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "prettier": "^3.2.4",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.3.3",
    "vite": "^7.3.1"
  },
  "engines": {
    "node": ">=18"
  }
}
